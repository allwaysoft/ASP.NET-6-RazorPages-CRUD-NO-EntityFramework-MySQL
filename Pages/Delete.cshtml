@page "{_id:int}"
@model RazorPages_CRUD_NO_EntityFramework.Pages.DeleteModel
@{
    ViewData["Title"] = "Delete";
}

<h2>删除学生</h2>
<hr />
<h4>是否要删除学生： <b>@Html.DisplayFor(Model=>Model.ListStudents.name)</b> ?</h4>
<form method="post">
